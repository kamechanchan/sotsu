<?xml version="1.0"?>
<launch>
  <arg name="num_dataset" default="10" />

 <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name" value="$(find gen_dataset)/worlds/empty.world"/>
  </include>
  <node name="setup_commands" pkg="timed_roslaunch" type="timed_roslaunch.sh" args="3 gen_dataset setup_commands.launch" output="screen" />

  <arg name="gui" default="true"/>
  <param name="use_gui" value="$(arg gui)"/>
  <node name="rviz" pkg="rviz" type="rviz" args="-d $(find gen_dataset)/rviz/generator_config.rviz" required="true" />
  <include file="$(find gen_dataset)/launch/gen_multi_photoneo.launch" />

</launch>
