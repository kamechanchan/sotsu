<?xml version="1.0"?>
<launch>
  <arg name="object_name" default="HV8" />
  <arg name="object_up" default="HV8_up" />
  <arg name="object_type" default="denso_parts" />
  <arg name="sensor_parent_frame" default="world" />
  <arg name="init_pose_up" default="-x 0 -y 0 -z 0" />

  <arg name="object_model" value="$(find denso_gazebo)/urdf/objects/$(arg object_type)/$(arg object_name)_1.urdf.gazebo.xacro" />
  <param name="object_description" type="str" command="$(find xacro)/xacro --inorder $(arg object_model)"/>
  <node name="spawn_object_urdf_up" pkg="gazebo_ros" type="spawn_model" args="-param object_description -urdf $(arg init_pose_up) -model $(arg object_name)" respawn="false" output="screen" />

</launch>
