<?xml version="1.0"?>
<launch>
    <arg name="service_name" default="six_input"/>
    <arg name="object_name" default="HV8"/>
    <arg name="object_count" default="25"/>

    <node pkg="ishiyama_annotation" type="move_object_numselect.py" name="move_object" output="screen">
        <param name="service_name" value="$(arg service_name)"/>
        <param name="object_name" value="$(arg object_name)"/>
        <param name="object_count" value="$(arg object_count)"/>
    </node>
</launch>