<?xml version="1.0"?>
<launch>
    <arg name="sensor_topic_name" default="all_cloud"/>
    <arg name="publish_topic_name" default="dummy_cloud"/>
    <arg name="the_number_of_object" default="26"/>
    
    
    <node pkg="annotation_package" type="pcl_baikai" name="pcl_baikai" output="screen">
        <param name="sensor_topic_name" value="$(arg sensor_topic_name)"/>
        <param name="publish_topic_name" value="$(arg publish_topic_name)"/>
        <param name="the_number_of_object" value="$(arg the_number_of_object)"/>
    </node>
</launch>