<?xml version="1.0"?>
<robot name="tercero" xmlns:xacro="http://ros.org/wiki/xacro">
  <xacro:include filename="$(find tercero_description)/robots/tercero.urdf.xacro"/>
  <xacro:include filename="$(find tercero_gazebo)/robots/tercero.gazebo.xacro"/>
